<h1>Games</h1>

<div class="product-container">
#foreach ($game in $games)
    <a href="/games/$game.getId()">
 <div class="product-list-show-cover">
     <img class="product-resize-for-list" src="$game.getGameImage()" alt="">
 </div></a>
    <p><a href="/games/$game.getId()">$game.getName()</a></p>
#end
</div>